<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="http://gtri.gatech.edu"?>
<params>
  <library>SimpleINS_plugin</library>

  <!-- noise params are defined with (mean, standard deviation) -->
  <pos_noise_0>0.0 5.0</pos_noise_0> <!-- x position noise -->
  <pos_noise_1>0.0 5.0</pos_noise_1> <!-- y position noise -->
  <pos_noise_2>0.0 1.0</pos_noise_2> <!-- z position noise -->

  <!-- velocity params are defined with (mean, standard deviation) -->
  <vel_noise_0>0.0 5.0</vel_noise_0> <!-- x velocity noise -->
  <vel_noise_1>0.0 5.0</vel_noise_1> <!-- y velocity noise -->
  <vel_noise_2>0.0 1.0</vel_noise_2> <!-- z velocity noise -->

 <!-- orient params are defined with (mean, standard deviation) -->
  <orient_noise_0>0.0 5.0</orient_noise_0> <!-- x orient noise -->
  <orient_noise_1>0.0 5.0</orient_noise_1> <!-- y orient noise -->
  <orient_noise_2>0.0 1.0</orient_noise_2> <!-- z orient noise -->

  <sea_state_gps>5.0</sea_state_gps> <!-- SeaState Condition for GPS Fix -->

</params>
